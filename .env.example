# Example .env file for Google Calendar MCP Agent

# OpenAI API Key (for LLM)
OPENAI_API_KEY=sk-...your-openai-api-key...

# LangSmith Configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=lsv2_...your-langsmith-api-key...
LANGCHAIN_PROJECT=google_calendar_mcp_agent

# (Optional) Google Cloud/VertexAI settings
# GOOGLE_GENAI_USE_VERTEXAI="True"
# GOOGLE_APPLICATION_CREDENTIALS="C:\path\to\your\google-credentials.json"
# GOOGLE_CLOUD_PROJECT="your-gcp-project-id"
# GOOGLE_CLOUD_LOCATION="us-central1"
